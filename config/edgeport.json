{
  "kind": "EdgePort",
  "apiVersion": "v2draft1",
  "metadata": {
    "ref": "ep001",
    "region": "us-east1"
  },
  "spec": {
    "bindAddr": "0.0.0.0",
    "securityContext": {
      "client": {
        "protocols": [
          "SSLv3",
          "TLSv1.2"
        ],
        "authType": "DisabledAll"
      },
      "keyStorePassword": "changeit",
      "trustStorePassword": "changeit",
      "keyStore": "/etc/routr/domains-cert.jks",
      "trustStore": "/etc/routr/domains-cert.jks",
      "keyStoreType": "pkcs12"
    },
    "processor": {
      "addr": "dispatcher:51901"
    },
    "externalIps": [
      "47.132.130.31"
    ],
    "localnets": [
      "192.168.1.3"
    ],
    "methods": [
      "REGISTER",
      "MESSAGE",
      "INVITE",
      "ACK",
      "BYE",
      "CANCEL"
    ],
    "unknownMethodAction": "Discard",
    "transport": [
      {
        "protocol": "tcp",
        "port": 5060
      },
      {
        "port": 5060,
        "protocol": "udp"
      },
      {
        "port": 5061,
        "protocol": "tls"
      },
      {
        "port": 5062,
        "protocol": "ws"
      },
      {
        "port": 5063,
        "protocol": "wss"
      }
    ]
  }
}