{
  "kind": "EdgePort",
  "apiVersion": "v2draft1",
  "metadata": {
    "ref": "ep001",
    "region": "us-east1"
  },
  "spec": {
    "bindAddr": "192.168.1.3",
    "securityContext": {
      "client": {
        "protocols" : ["SSLv3", "TLSv1.2", "TLSv1.1", "TLSv1"],
        "authType": "DisabledAll"
      },
      "keyStorePassword": "changeit",
      "trustStorePassword": "changeit",
      "keyStore": "etc/certs/domains-cert.jks",
      "trustStore": "etc/certs/domains-cert.jks",
      "keyStoreType": "pkcs12"
    },
    "processor": {
      "addr": "localhost:51901"
    },
    "externalIps": ["47.132.130.31"],
    "localnets": ["192.168.1.9"],
    "methods": [
      "INVITE",
      "MESSAGE",
      "REGISTER"
    ],
    "transport": [
      {
        "protocol": "tcp",
        "port": 5060
      },
      {
        "port": 5060,
        "protocol": "udp"
      }
    ]
  }
}