{
  "kind": "Location",
  "apiVersion": "v2draft1",
  "metadata": {
    "region": "us-east1"
  },
  "spec": {
    "bindAddr": "0.0.0.0:51905",
    "cache" :{
      "provider": "memory"
    },
    "backends": [
      {
        "ref": "voice"
      },
      {
        "ref": "conference",
        "balancingAlgorithm": "least-sessions",
        "sessionAffinity": {
          "enabled": true,
          "ref": "room_id"
        }
      }
    ]    
  }
}
